#include <iostream>
using namespace std;

void input(int s[][3]);//可以没有行，但是必须有列

void output(int s[][3]);

int main() {
    int a[2][3];
    input(a);
    output(a);
}

void input(int s[][3]) {
    for (int i = 0; i < 2; i++)
        for (int j = 0; j < 2; j++) {
            cin >> s[i][j];
        }
}

void output(int s[][3]) {
    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 2; j++) {
            cout << s[i][j] << '\t';
        }
        cout << endl;
    }
}
